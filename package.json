{
  "name": "codexdock",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "node scripts/dev.mjs",
    "dev:server": "tsx watch server/index.ts",
    "dev:web": "vite",
    "build": "pnpm run build:web && pnpm run build:server",
    "build:web": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "start": "node scripts/start.mjs",
    "up": "rm -rf dist && pnpm build && pnpm start",
    "lint": "biome lint .",
    "format": "biome check --write .",
    "check": "pnpm format && pnpm typecheck && pnpm test",
    "typecheck": "tsgo --noEmit -p tsconfig.app.json && tsgo --noEmit -p tsconfig.server.json",
    "test": "vitest --run"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "hono": "^4.5.4",
    "get-port": "^7.1.0",
    "pino": "^8.21.0",
    "pino-http": "^9.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.0",
    "sonner": "^2.0.7",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.12",
    "@typescript/native-preview": "^7.0.0-dev.20250906.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "volta": {
    "node": "22.22.0"
  }
}
